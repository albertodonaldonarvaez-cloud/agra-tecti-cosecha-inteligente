# ============================================
# CONFIGURACIÓN DE BASE DE DATOS
# ============================================

# Credenciales de MySQL para Docker Compose
MYSQL_ROOT_PASSWORD=change_this_root_password
MYSQL_DATABASE=agratec_db
MYSQL_USER=agratec_user
MYSQL_PASSWORD=change_this_user_password

# URL de conexión a la base de datos
DATABASE_URL=mysql://agratec_user:change_this_user_password@db:3306/agratec_db

# ============================================
# CONFIGURACIÓN DE LA APLICACIÓN
# ============================================

NODE_ENV=production
VITE_APP_TITLE=Agratec - Dashboard de Cosecha de Higo
VITE_APP_LOGO=/logo.svg

# ============================================
# AUTENTICACIÓN Y SEGURIDAD
# ============================================

JWT_SECRET=change_this_to_a_random_secret_at_least_32_characters_long

# ============================================
# OAUTH Y MANUS
# ============================================

VITE_APP_ID=your_manus_app_id
OAUTH_SERVER_URL=https://api.manus.im
VITE_OAUTH_PORTAL_URL=https://portal.manus.im
OWNER_OPEN_ID=your_owner_open_id
OWNER_NAME=Your Name

# ============================================
# APIS DE MANUS (OPCIONAL)
# ============================================

BUILT_IN_FORGE_API_URL=https://forge.manus.im
BUILT_IN_FORGE_API_KEY=your_backend_forge_api_key
VITE_FRONTEND_FORGE_API_KEY=your_frontend_forge_api_key
VITE_FRONTEND_FORGE_API_URL=https://forge.manus.im

# ============================================
# ANALYTICS (OPCIONAL)
# ============================================

VITE_ANALYTICS_ENDPOINT=your_analytics_endpoint
VITE_ANALYTICS_WEBSITE_ID=your_website_id
